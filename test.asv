global a b t

list =get(handles.listbox1,'value');

switch list
    case 1
        constant_epsilon_greedy()
        axes(handles.axes1)
        plot(t,a)
        title('Greedy Reward')
        xlabel('steps')
        ylabel('Average reward')

        axes(handles.axes2)
        plot(t,b)
        title('Greedy Optimal Action')
xlabel('steps')
        ylabel('%Optimal Actoin')
    case 2
        constant_Softmax()
    case 3
        exponential_variable_epsilon_greedy()
    case 4
        exponential_variable_Tau_Softmax()
    case 5
        fraction_variable_epsilon_greedy()
    case 6
        fraction_variable_Tau_Sofmax()
    case 7
        greedy()
    case 8
        Optimistic_Initial_Values()
end

